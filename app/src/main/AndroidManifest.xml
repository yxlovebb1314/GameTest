        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BallWithFingerActivity" />
        <activity android:name=".activity.BallWithFingerSurfaceActivity" />
        <activity android:name=".activity.MyBitmapViewActivity" />
        <activity android:name=".activity.ClipCanvasActivity" />
        <activity android:name=".activity.AnimationViewActivity" />
        <activity android:name=".activity.BitmapMovieActivity" />
        <activity android:name=".activity.FrameMovieActivity" />
        <activity android:name=".activity.PlayerActivity" />
        <activity android:name=".activity.RectCollisionActivity" />
        <activity android:name=".activity.RegionCollisionActivity" />
        <activity android:name=".activity.SoundPoolActivity" />
        <activity
            android:name=".activity.ControlBarActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.PlayerMoveWithControlBarActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".test.PlaneGame.PlaneGameActivity" />
        <activity android:name=".activity.BigBitmapActivity" />
        <activity android:name=".activity.BigBitmap2Activity"></activity>
    </application>

</manifest>